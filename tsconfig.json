{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "esModuleInterop": true,
    "incremental": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "target": "ES2019",//Align with Jupyterlite
    "types": [] ,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": false //true,
    // "inlineSourceMap": false,
    // "sourceRoot": "./src"
  },
  "include": ["src/*","src/types/*", "src/Tobedeleted/index.ts", "src/Tobedeleted/worker.ts", "src/Tobedeleted/kernel.ts"]
}

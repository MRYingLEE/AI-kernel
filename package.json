{
  "name": "@jupyterlite/ai-kernel",
  "version": "1.0.0",
  "description": "An AI kernel for JupyterLite",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlite",
    "jupyterlite-extension"
  ],
  "homepage": "https://github.com/MRYingLEE/ai-kernel",
  "bugs": {
    "url": "https://github.com/MRYingLEE/ai-kernel/issues"
  },
  "author": {
    "name": "Ying li",
    "email": "Mr.Ying.LEE@Gmail.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts, src/types/handlebars1.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/MRYingLEE/AI-kernel.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyterlite_ai_kernel/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jlpm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@azure/communication-chat": "^1.3.2",
    "@azure/identity": "^3.3.2",
    "@azure/openai": "^1.0.0-beta.7",
    "@jupyterlab/coreutils": "~6.0.7",
    "@jupyterlab/services": "^7.0.7",
    "@jupyterlite/kernel": "^0.2.0",
    "@jupyterlite/server": "^0.2.0",
    "comlink": "^4.3.1",
    "easy-speech": "^2.1.1",
    "exponential-backoff": "^3.1.1",
    "handlebars": "^4.7.8",
    "ts": "^0.0.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@types/json-schema": "^7.0.11",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "source-map-loader": "^1.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "~5.0.2",
    "yjs": "^13.5.0",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.12.1",
    "@jupyterlab/testutils": "~4.0.7",
    "@types/jest": "^29.5.3",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlite_ai_kernel/labextension",
    "sharedPackages": {
      "@jupyterlite/kernel": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "jupyterlite": {
    "liteExtension": true
  },
  "jupyter-releaser": {
    "skip": [
      "check-links"
    ],
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab~=3.1",
        "jlpm"
      ]
    }
  }
}

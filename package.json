{
  "name": "@jupyterlite/ai-kernel",
  "version": "0.8.0",
  "description": "An AI kernel for JupyterLite",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlite",
    "jupyterlite-extension"
  ],
  "homepage": "https://github.com/MRYingLEE/ai-kernel",
  "bugs": {
    "url": "https://github.com/MRYingLEE/ai-kernel/issues"
  },
  "author": {
    "name": "Ying li",
    "email": "Mr.Ying.LEE@Gmail.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts, src/types/handlebars1.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/MRYingLEE/AI-kernel.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyterlite_ai_kernel/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jlpm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@azure/communication-chat": "^1.3.2",
    "@azure/identity": "^3.3.2",
    "@azure/openai": "^1.0.0-beta.7",
    "@jupyterlab/services": "^7.0.7",
    "@jupyterlite/kernel": "^0.2.0",
    "@jupyterlite/server": "^0.2.0",
    "easy-speech": "^2.1.1",
    "exponential-backoff": "^3.1.1",
    "handlebars": "^4.7.8",
    "ts": "^0.0.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlite_ai_kernel/labextension",
    "sharedPackages": {
      "@jupyterlite/kernel": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "jupyterlite": {
    "liteExtension": true
  },
  "jupyter-releaser": {
    "skip": [
      "check-links"
    ],
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab~=3.1",
        "jlpm"
      ]
    }
  }
}
